    "lodash": "^4.17.4",
    "moment": "^2.22.0",
    "mongod": "^2.0.0",
    "mongoose": "^4.11.2",
    "redux-devtools-extension": "^2.13.2",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
