  "main": "server/server.js",
  "engines": {
    "node": "7.7.4",
    "npm": "5.0.4"
  },
  "jest": {
    "testRegex": "./tests/.*.js$",
    "rootDir": "."
  },
  "scripts": {
    "start": "node dist/server.js",
    "server": "nodemon server/server.js --exec babel-node",
    "build": "babel server -d dist",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
